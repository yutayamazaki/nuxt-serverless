{
  "name": "nuxt_serverless",
  "version": "1.0.0",
  "description": "Nuxt + Lambda template",
  "author": "yutayamazaki",
  "private": true,
  "scripts": {
    "dev": "node ./server.js",
    "build": "NODE_ENV=production nuxt build",
    "postbuild": "webpack",
    "start": "NODE_ENV=production node ./.nuxt/dist/server.js",
    "generate": "nuxt generate",
    "deploy:dev": "sls deploy --stage dev",
    "deploy:stage": "NODE_ENV=production sls deploy --stage stage",
    "deploy:prod": "NODE_ENV=production sls deploy --stage prod",
    "undeploy:dev": "sls remove --stage dev",
    "undeploy:stage": "NODE_ENV=production sls remove --stage stage",
    "undeploy:prod": "NODE_ENV=production sls remove --stage prod"
  },
  "dependencies": {
    "nuxt": "^2.12.2"
  },
  "devDependencies": {
    "aws-serverless-express": "^3.3.8",
    "express": "^4.17.1",
    "express-asyncify": "^1.0.1",
    "serverless": "^1.67.1",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-dotenv-plugin": "^2.3.2",
    "serverless-s3-sync": "^1.12.0",
    "vue": "^2.6.11",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpackbar": "^4.0.0"
  }
}
